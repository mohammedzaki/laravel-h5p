{
  "name": "mohammed-zaki/laravel-h5p",
  "description": "H5P Plugin for laravel platform",
  "type": "library",
  "authors": [
    {
      "name": "Mohammed Zaki",
      "email": "mohammedzaki.dev@gmail.com"
    },
    {
      "name": "Alefe Souza",
      "email": "contact@alefesouza.com"
